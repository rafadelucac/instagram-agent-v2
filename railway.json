{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "DOCKERFILE"
    },
    "deploy": {
      "startCommand": null,
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 1,
      "sleepApplication": true,
      "limitOverride": {
        "containers": {
          "cpu": 1,
          "memoryBytes": 268435456
        }
      }
    }
  }